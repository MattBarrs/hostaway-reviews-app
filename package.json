{
  "name": "hostaway-reviews-app",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "server:build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "server:start": "node --loader ts-node/esm --no-warnings=ExperimentalWarning server.ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^5.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.4",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.9",
    "http-proxy-middleware": "^3.0.5",
    "prettier": "^3.7.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
